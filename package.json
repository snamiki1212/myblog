{
  "name": "gatsby-lunash-blog",
  "description": "Lunash Blog",
  "version": "1.2.0",
  "author": "Shun Namiki <snamiki1212@gmail.com>",
  "scripts": {
    "start": "gatsby develop --verbose",
    "develop": "gatsby develop",
    "dev": "yarn run develop",
    "serve": "gatsby serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ci": "CI=true yarn gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "clean:force": "yarn clean && rm -rf node_modules",
    "lint:ts": "eslint --ext .ts,.tsx .",
    "lint:js": "eslint --ext .js,.jsx .",
    "format:js": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "format:md": "prettier '**/*.{md}' --write",
    "precommit": "lint-staged",
    "lint": "eslint --ext ./ 'src/**/*.{js,jsx,ts,tsx}'",
    "tlint": "textlint contents/**/index.md",
    "type-check": "tsc --noEmit",
    "test": "jest test",
    "git:merge:starter/master": "git merge starter/master --no-ff --no-commit"
  },
  "dependencies": {
    "//": "rehype-react: ver4系でないとうまく動かない。ver6系だとunifiedを組み合わせないといけなくて、その上で現状の動きを再現するのがよくわからん、軽く触った出来るような気もするけど後回しにしてる。ver5系はチェックしてない。",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/icons": "^4.11.2",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "gatsby": "^3.9.1",
    "gatsby-plugin-catch-links": "3.1.0",
    "gatsby-plugin-feed": "3.1.0",
    "gatsby-plugin-google-analytics": "3.1.0",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-manifest": "3.1.0",
    "gatsby-plugin-nprogress": "3.1.0",
    "gatsby-plugin-offline": "4.1.0",
    "gatsby-plugin-react-helmet": "4.1.0",
    "gatsby-plugin-sass": "4.1.0",
    "gatsby-plugin-sharp": "^3.1.0",
    "gatsby-plugin-sitemap": "3.1.0",
    "gatsby-plugin-twitter": "3.1.0",
    "gatsby-plugin-typescript": "^3.1.0",
    "gatsby-remark-autolink-headers": "3.1.0",
    "gatsby-remark-copy-linked-files": "3.1.0",
    "gatsby-remark-images": "4.1.0",
    "gatsby-remark-prismjs": "4.1.0",
    "gatsby-remark-relative-images": "2.0.2",
    "gatsby-remark-responsive-iframe": "3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-transformer-remark": "2.8.28",
    "gatsby-transformer-sharp": "^3.1.0",
    "lodash.kebabcase": "^4.1.1",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "prismjs": "^1.24.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0",
    "rehype-react": "^4.0.1",
    "remark-parse": "^8.0.3",
    "sharp": "^0.27.2",
    "styled-components": "^5.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "unified": "^9.1.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-gatsby": "^0.9.1",
    "cli-glob": "^0.1.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-remark-graphviz": "^1.3.10",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^17.0.1",
    "remark-cli": "^8.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "source-map-support": "^0.5.19",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "textlint": "^11.7.6",
    "textlint-rule-preset-japanese": "^5.0.0",
    "textlint-rule-preset-jtf-style": "^2.3.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
